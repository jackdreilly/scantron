rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /fcm_tokens/{user_id} {
      allow read: if false;
      allow write: if request.auth != null && request.auth.uid == user_id
    }
    match /scanlets/{scanlet_id} {
      allow read;
      allow write: if request.auth != null;
    }
  }

}